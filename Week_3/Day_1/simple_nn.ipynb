    "    def sigmoid(self,x):\n",
    "        return (1/(1+np.exp(-x)))\n",
    "    \n",
    "    def forward(self,x):\n",
    "        self.z2 = np.dot(x,w2)\n",
    "        self.a2 = sigmoid(self.z2)\n",
    "#         print(self.a2)\n",
    "        self.z3 = np.dot(self.a2,w3)\n",
    "        yHat = sigmoid(self.z3)\n",
    "        print(yHat)\n",
    "        return yHat\n",
    "        \n",
    "    def costFunction(self, x,y):\n",
    "        return (x-y)**2\n",
    "    \n",
    "    def meanSquaredError(self,y):\n",
    "        return ((y-self.forward(x))*(y-self.forward(x)))/2\n",
    "    \n",
    "\n",
